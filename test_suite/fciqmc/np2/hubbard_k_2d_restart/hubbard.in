np2_id = 1
redistribute { nprocs = 2, read = np1_id, write = np2_id }

fciqmc {
    sys = sys,
    qmc = {
        tau = 0.01,
        rng_seed = 7,
        init_pop = 10,
        mc_cycles = 25,
        nreports = 1000,
        target_population = 5500,
        state_size = -500,
        spawned_state_size = -200,
    },
    restart = { read = np2_id },
}
