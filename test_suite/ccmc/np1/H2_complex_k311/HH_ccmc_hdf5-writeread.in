system = {
    int_file = "fcidumpfile",
    complex = true,
    ex_int_file = "fcidumpfile_X",
}
sys = read_in(system)

hdf5_file = write_read_in_system {
    sys = sys,
    }

sys:free()
system.int_file = hdf5_file

-- not required as ex_int_file is ignored for a .H5 files, but just to check it works.
system.ex_int_file = nil

sys = read_in(system)

ex_l=2
ccmc {
    sys = sys,
    qmc = {
        tau = 0.01,
        rng_seed = 13086,
        mc_cycles = 5,
        init_pop = 500,
        nreports =  40,
        target_population = 20000,
        state_size = -500,
        spawned_state_size = -200,
        quasi_newton = true,
        quasi_newton_threshold = 1e-5, 
        shift_damping = 0.3,
        rng_seed= 5,
    },
    ccmc = {
            even_selection = true,
            full_non_composite=true,
           },
    reference = {
        ex_level = ex_l,
    },
}
