sys = read_in {
    int_file = "INTDUMP",
    sym = 'aufbau',
    Lz = true,
}

psip_list = mp1_mc {
    sys = sys,
    mp1 = {
        D0_population = 2000,
        state_size = -2000,
        real_amplitudes = true,
        spawn_cutoff = 0.001,
    }
}

ccmc {
    sys = sys,
    psip_list = psip_list,
    qmc = {
        rng_seed = 12345,
        tau = 0.01,
        real_amplitudes = true,
        init_pop = 2000,
        mc_cycles = 2,
        nreports = 100,
        target_population = 5e6,
        state_size = -2000,
        spawned_state_size = -2000,
        vary_shift = false,
        vary_shift_from = 'proje',
        shift_damping = 0.01,
    	excit_gen = 'heat_bath',
	    quasi_newton = true,
        },
    reference = {
        ex_level = 2,
        },
    ccmc = {
        full_non_composite = true,
        even_selection = false,
        cluster_multispawn_threshold = 5,
        multiref = true,
        use_mr_proj_energy = true,
        mr_proj_energy_n_refs = 4,
        mr_proj_energy_warmup_iter = 300,
        mr_acceptance_search = 'bk_tree',
        n_secondary_ref = 4899,
        mr_read_in = true,
        mr_n_frozen = 4,
        mr_excit_lvl = 2,
        mr_secref_file = 'secondary_references'
     },
}

