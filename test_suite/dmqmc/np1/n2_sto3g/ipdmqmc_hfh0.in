sys = read_in {
    int_file = "FCIDUMP",
    nel = 10,
    ms = 0,
    sym = 0,
}

dmqmc {
    sys = sys,
    qmc = {
        tau = 0.001,
        rng_seed = 7,
        init_pop = 1000,
        mc_cycles = 10,
        nreports = 100,
        target_population = 1e3,
        state_size = -200,
        spawned_state_size = -100,
        initial_shift = -20,
    },
    dmqmc = {
        all_sym_sectors = true,
        beta_loops = 10,
    },
    ipdmqmc = {
        target_beta = 1.0,
        symmetric = false,
        initial_matrix = 'free_electron',
        grand_canonical_initialisation = true,
        check_reference = false,
    },
    operators = {
        energy = true,
        H0_energy = true,
    },
}
