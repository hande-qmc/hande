sys = read_in {
    int_file = "FCIDUMP",
    nel = 10,
    ms = 0,
    sym = 0,
}

dmqmc {
    sys = sys,
    qmc = {
        tau = 0.001,
        rng_seed = 7,
        init_pop = 1000,
        mc_cycles = 10,
        real_amplitudes = true,
        real_amplitude_force_32 = true,
        nreports = 100,
        target_population = 1e10,
        state_size = -200,
        spawned_state_size = -100,
    },
    dmqmc = {
        all_sym_sectors = true,
        all_spin_sectors = true,
        beta_loops = 10,
    },
    ipdmqmc = {
        target_beta = 1.0,
        initial_matrix = 'hartree_fock',
        grand_canonical_initialisation = true,
        symmetric = false,
        check_reference = false,
        count_reweighted_particles = false,
    },
    operators = {
        energy = true,
    },
}
